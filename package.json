{
  "name": "VendingMachine",
  "version": "1.0.0",
  "description": "API for a vending machine",
  "main": "index.ts",
  "scripts": {
    "test": "jest --coverage=false --verbose --detectOpenHandles --forceExit --runInBand",
    "lint": "eslint . --ext .ts",
    "docker-start": "docker-compose -f docker-compose.dev.yml up -d --build && docker-compose -f docker-compose.dev.yml logs -f && ts-node-dev --respawn --pretty --files --watch src src/index.ts",
    "dev": "ts-node-dev --respawn --pretty --files --watch src src/index.ts",
    "down": "docker-compose -f docker-compose.dev.yml down",
    "start": "node dist/index.js",
  },
  "repository": {
    "type": "git",
