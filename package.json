    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.0",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "bun-types": "latest",
    "eslint": "8.56.0",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/jsonwebtoken": "9.0.5",
